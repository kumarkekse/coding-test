<%= form_tag "/home/customers",method: :get,remote:true do  %>  
  <div class="container">    
    <div class="row">
      <div class="col-sm-6 form-group">
        <div class="input-group" id="DateDemo">
          <input type='text' name="daterange" id='weeklyDatePicker' placeholder="Select Week" />
        </div>
      </div>
    </div>
  </div>   
<% end %>
<table class="table table-inverse table-bordered">
  <thead>
    <tr>
      <th>Orders</th>
      <th>Customers</th>
      <th>Percentage</th>
    </tr>
  </thead>
  <tbody class="customers-orders">
    <%=render 'customer_orders'%>
  </tbody>
</table>

<table class="table table-inverse table-bordered">
  <thead>
    <tr>
      <th>Month</th>
      <th>Reccuring Customer</th>
      <th>New Customer</th>
      <th>Total</th>
    </tr>
  </thead>
  <tbody class="reccuring-data">
    <%= render 'monthly_reccuring_customers'%>
  </tbody>
</table>
<script type="text/javascript">
  $('#weeklyDatePicker').datetimepicker({
    format: 'DD-MMM-YYYY'
  });
</script>